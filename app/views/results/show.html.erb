<% provide(:title, "View Result") %>
<h1><%= @participant.name %></h1>
<h3>Results</h3>

<div class="row">
  <table>
    <thead>
      <th class="yearColumn">Year</th>
      <th class="digitColumn">Round</th>
    </thead>
    <tbody>
      <% for result in @participant.results %>
      <tr>
        <td class="yearColumn"><%= result.year %></td>
        <td class="digitColumn"><%= result.round %></td>                                        
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
